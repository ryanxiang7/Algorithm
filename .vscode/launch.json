	"version": "0.2.0",
	"configurations": [
		{
			"name": "jsdebug",
			"program": "${workspaceFolder}\\${relativeFile}",
			"request": "launch",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		},

		{
			"type": "lldb",
			"request": "launch",
			"name": "rustDebug",
			"program": "${workspaceFolder}/Rust/target/debug/Rust.exe",
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}
